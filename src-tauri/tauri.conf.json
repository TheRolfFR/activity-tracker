{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run bindings && npm run build",
    "devPath": "http://localhost:5173",
    "distDir": "../build/",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "activity-tracker",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": true
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.therolf.activitytracker",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "systemTray": {
      "iconPath": "../src/lib/assets/icon.png",
      "iconAsTemplate": true
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 385,
        "resizable": false,
        "title": "Work tracker",
        "width": 500,
        "decorations": false,
        "skipTaskbar": true,
        "alwaysOnTop": true,
        "transparent": true,
        "visible": false
      }
    ]
  }
}
